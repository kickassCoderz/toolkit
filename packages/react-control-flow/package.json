{
    "name": "@kickass-coderz/react-control-flow",
    "version": "0.0.0",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./for": {
            "types": "./dist/for.d.ts",
            "import": "./dist/for.js"
        },
        "./show": {
            "types": "./dist/show.d.ts",
            "import": "./dist/show.js"
        },
        "./matcher": {
            "types": "./dist/matcher.d.ts",
            "import": "./dist/matcher.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "check-types": "tsc --noEmit",
        "dev": "tsup --watch",
        "lint": "eslint . --max-warnings 0"
    },
    "devDependencies": {
        "@kickass-coderz/eslint-config": "workspace:*",
        "@kickass-coderz/ts-config": "workspace:*"
    },
    "peerDependencies": {
        "@types/react": ">= 18 <19",
        "react": ">=18 <19"
    },
    "publishConfig": {
        "access": "public"
    }
}
